{
"agent": {
"metrics_collection_interval": 60,
"run_as_user": "root"
},
"logs": {
"logs_collected": {
"files": {
"collect_list": [
{
"file_path": "/var/log/secure",
"log_group_name": "/ec2/var/log",
"log_stream_name": "{instance_id}/secure",
"retention_in_days": -1
},
{
"file_path": "/var/log/messages",
"log_group_name": "/ec2/var/log",
"log_stream_name": "{instance_id}/messages",
"retention_in_days": -1
}
]
}
}
},
"metrics": {
"aggregation_dimensions": [
[
"InstanceId"
]
],
"append_dimensions": {
"AutoScalingGroupName": "",
"ImageId": "",
"InstanceId": "",
"InstanceType": ""
},
"metrics_collected": {
"cpu": {
"measurement": [
"cpu_usage_idle",
"cpu_usage_iowait",
"cpu_usage_user",
"cpu_usage_system"
],
"metrics_collection_interval": 60,
"resources": [
"*"
],
"totalcpu": false
},
"disk": {
"measurement": [
"used_pernt",
"inodes_free"
],
"metrics_collection_interval": 60,
"resources": [
"*"
]
},
"di"di"di"di"di"di"d"io_time"
],
"metrics_collection_interval": 60,
"resources": [
"*"
]

"mem": {
"measurement": [
"mem_"mem_"mem_"mem_"meec"mem_"mem_"mem_"mem_"metrics_aggregation_interval": 60,
etcs_collection_interval": 10,
"service_address": ":8125"
},
},
ervice_ad"measervment": [
"swap_used_percent"
],
"metrics_collection_interval": 60
}
}
}
}
